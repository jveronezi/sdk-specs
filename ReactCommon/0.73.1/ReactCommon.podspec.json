    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "GCC_WARN_PEDANTIC": "YES"
  },
  "dependencies": {
    "React-logger": ["0.73.1"]
  },
  "subspecs": [
    {
      "name": "turbomodule",
      "dependencies": {
        "React-callinvoker": ["0.73.1"],
        "React-perflogger": ["0.73.1"],
        "React-cxxreact": ["0.73.1"],
        "React-jsi": ["0.73.1"],
        "RCT-Folly": ["2022.05.16.00"],
        "DoubleConversion": [],
        "fmt": ["~> 6.2.1"],
        "glog": [],
        "hermes-engine": []
      },
      "subspecs": [
        {
          "name": "bridging",
          "dependencies": {
            "React-jsi": ["0.73.1"],
            "hermes-engine": []
          },
          "source_files": "packages/react-native/ReactCommon/react/bridging/**/*.{cpp,h}",
          "exclude_files": "packages/react-native/ReactCommon/react/bridging/tests",
          "header_dir": "packages/react-native/ReactCommon/react/bridging",
          "pod_target_xcconfig": {
            "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/ReactCommon\" \"$(PODS_ROOT)/RCT-Folly\""
          }
        },
        {
          "name": "core",
          "source_files": [
            "packages/react-native/ReactCommon/react/nativemodule/core/ReactCommon/**/*.{cpp,h}",
            "packages/react-native/ReactCommon/react/nativemodule/core/platform/ios/**/*.{mm,cpp,h}"
          ]
        }
      ]
    }
  ]
}
